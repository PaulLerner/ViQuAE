                        "settings": {
                            "similarity": {
                                "viquae_bm25": {
                                    "b": 0.3,
                                    "k1": 0.5
                                }
                            }
                        },
                        "mappings": {
                            "properties": {
                                "text": {
                                    "type": "text",
                                    "analyzer": "standard",
                                    "similarity": "viquae_bm25"
                                }
                            }
                        }
                    }
                }
            }
        },
        "data/viquae_wikipedia/non_humans": {
