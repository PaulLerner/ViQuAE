                },
                "clip-RN50": {
                    "column": "clip-RN50",
                    "es": false,
                    "kind_str": "IMAGE",
                    "key": "clip-RN50",
                    "string_factory": "L2norm,Flat",
                    "load": false,
                    "device": null,
                    "metric_type": 0
                }
            }
        },
        "data/viquae_wikipedia/humans_with_faces": {
            "index_mapping_path": "data/viquae_wikipedia/humans_with_faces/article2passage.json",
            "load_dataset": true,
            "index_kwargs": {
